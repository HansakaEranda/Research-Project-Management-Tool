{
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": ["react", "jsx-a11y", "import"],
    "rules":{
        "react/prop-types": 0,
        "react/react-in-jsx-scope":0
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx":true
        }
    },
    "env": {
        "es6":true,
        "browser":true,
        "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}